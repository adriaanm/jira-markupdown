  type Self >: this.type <: A
}

case class W(f: A) { outer =>
  type F = f.Self
  def selfF: F = f: f.Self
  type Self = W {
    type F = outer.F
  }

  def error(w: Self) = {
    val wF: w.F = w.selfF
  }
}

