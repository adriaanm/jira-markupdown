{code:title=Reproducable|borderStyle=solid}
    trait Foo {
      val bar: Any
    }

    class Foo1 extends Foo {
      object bar
    }

    class Foo2 extends Foo {
      object bar
    }

    val foos/*: Seq[Foo]*/ = Seq(new Foo1, new Foo2)
    foos.map(_.bar)
{code}


