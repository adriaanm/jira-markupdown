sealed trait Foo[A]
case class Bar[A](f: A) extends Foo[A]

object Extractor {
  def unapply[A](f: Bar[_ >: A]): Option[A] = ??? // no crash if `f: Foo[_ >: A]`
}

object Main extends App {

  type X = Int

  val t: Foo[X] = ???

  t match {
    case Extractor(f) => f
    case _            => ???
  }
}
