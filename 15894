{code}
trait T1 {
  def f: Unit
}                                                                               

trait T2 extends T1 {
  abstract override def f: Unit = "in T1"
  def something = super.f  // So the "abstract override" is needed
}                                                                               

trait Q1 {
  def f: Unit = "bippy"
}                                                                               

trait T3 extends T2 with Q1 {
  abstract override def f: Unit = "in T2"
}

object Test {
  def main(args: Array[String]): Unit = {
    new T3 { }
  }
}
{code}
