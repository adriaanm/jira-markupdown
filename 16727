```scala
import scala.tools.nsc.reporters._
import scala.tools.nsc.Settings
import reflect.runtime.Mirror.ToolBox

class Foo extends StaticAnnotation

object Test extends App {
  println(scala.reflect.Code.lift{
    5: @Foo
  }.tree.toString)
}
