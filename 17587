This code works on Scala 2.9.2 and 2.10.0-M2, and crashes from 2.10.0-M3 to 2.10.0-RC1
{code}
import scala.util.continuations._

private object Foo {
  private class Bar
}

class Foo {
  import Foo.Bar

  private def bad(): Bar @suspendable = {
    "baz" match {
      case _ =>
        shift { (continue: Bar => Unit) => }
    }
  }
}
{code}
